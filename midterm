{
	//팩토리얼
	int n;
	int result = 1;
	printf("정수 입력: ");
	scanf("%d", &n);
	for (int i = 1; i <= n; i++)
	{
		result *= i;
		
	}
	printf("%d ! = %d", n, result);
}
